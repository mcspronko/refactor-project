{
    "require": {
        "league/route": "^3.0",
        "zendframework/zend-diactoros": "^1.6",
        "symfony/http-client": "^4.3",
        "symfony/finder": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/config": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "ext-json": "*",
        "zendframework/zend-httphandlerrunner": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.3",
        "squizlabs/php_codesniffer": "~3"
    },
    "scripts": {
        "tests": "vendor/bin/phpunit tests/",
        "test-coverage": "vendor/bin/phpunit ./tests/ --coverage-text=test_coverage_report.txt",
        "test-phpcs": "vendor/bin/phpcs --standard=PSR2 ./src/"
    },
    "autoload": {
        "psr-4": {
            "JournalMedia\\Sample\\": "src/",
            "JournalMedia\\Sample\\Test\\": "tests/"
        }
    }
}
